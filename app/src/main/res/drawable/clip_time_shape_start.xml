<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="14dp" android:height="14dp" android:gravity="center_horizontal">
        <shape android:shape="oval">
            <corners android:radius="7dp"/>
            <solid android:color="@color/white"/>
            <size android:width="14dp"
                android:height="14dp"/>
        </shape>
    </item>
    <item android:gravity="center_horizontal" android:width="1dp" android:height="39dp">
        <shape android:shape="rectangle">
            <corners android:radius="50dp"></corners>
            <solid android:color="@color/white"/>
            <size android:width="1dp" android:height="39dp"></size>
        </shape>
    </item>
</layer-list>